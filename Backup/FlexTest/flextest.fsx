let produceSql (instrument_str: string) (currency_str: string) flow_type =
    let spliter = [|'\n'|]
    let a1 = instrument_str.Split spliter
    let a2 = currency_str.Split spliter
    seq{
       for (i, a) in  Array.zip a1 a2 do
          yield sprintf "insert into AccountingMapping(MappingType,InstrumentId,CurrencyId,MT4LoginId) values (%d,'%s','%s',%d)" flow_type i a 111
    }
    


